{
  "openapi": "3.0.1",
  "info": {
    "title": "EventoApi",
    "version": "1.0"
  },
  "paths": {
    "/API/Evento/ListaEventos": {
      "get": {
        "tags": [
          "Evento"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/API/Evento/EventoPorId/{id}": {
      "get": {
        "tags": [
          "Evento"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/API/Evento/CrearEvento": {
      "post": {
        "tags": [
          "Evento"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/API/Evento/EditarEvento/{id}": {
      "put": {
        "tags": [
          "Evento"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Evento"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/API/Lugar/ListaLugares": {
      "get": {
        "tags": [
          "Lugar"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Ciudad": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "type": "string",
            "nullable": true
          },
          "provinciaId": {
            "type": "integer",
            "format": "int32"
          },
          "lugars": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Lugar"
            },
            "nullable": true
          },
          "provinciaIDNavegation": {
            "$ref": "#/components/schemas/Provincia"
          }
        },
        "additionalProperties": false
      },
      "DateOnly": {
        "type": "object",
        "properties": {
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "month": {
            "type": "integer",
            "format": "int32"
          },
          "day": {
            "type": "integer",
            "format": "int32"
          },
          "dayOfWeek": {
            "$ref": "#/components/schemas/DayOfWeek"
          },
          "dayOfYear": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "dayNumber": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "DayOfWeek": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5,
          6
        ],
        "type": "integer",
        "format": "int32"
      },
      "Evento": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "type": "string",
            "nullable": true
          },
          "fecha": {
            "$ref": "#/components/schemas/DateOnly"
          },
          "horarioInicio": {
            "$ref": "#/components/schemas/TimeOnly"
          },
          "horarioFin": {
            "$ref": "#/components/schemas/TimeOnly"
          },
          "descripcion": {
            "type": "string",
            "nullable": true
          },
          "lugarId": {
            "type": "integer",
            "format": "int32"
          },
          "lugarIDNavegation": {
            "$ref": "#/components/schemas/Lugar"
          }
        },
        "additionalProperties": false
      },
      "Lugar": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "descripcion": {
            "type": "string",
            "nullable": true
          },
          "ciudadId": {
            "type": "integer",
            "format": "int32"
          },
          "ciudadIDNavegation": {
            "$ref": "#/components/schemas/Ciudad"
          }
        },
        "additionalProperties": false
      },
      "Provincia": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "type": "string",
            "nullable": true
          },
          "ciudads": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Ciudad"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TimeOnly": {
        "type": "object",
        "properties": {
          "hour": {
            "type": "integer",
            "format": "int32"
          },
          "minute": {
            "type": "integer",
            "format": "int32"
          },
          "second": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "millisecond": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "microsecond": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "nanosecond": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "ticks": {
            "type": "integer",
            "format": "int64",
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}